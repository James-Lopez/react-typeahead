{
  "name": "react-typeahead",
  "version": "1.0.0",
  "description": "Application for autocompleting search terms.",
  "main": "app.js",
  "scripts": {
    "seed": "node server/seed.js",
    "bundle:watch": "watchify client/index.js -vd -t babelify -o server/public/bundle.js ",
    "start": "nodemon server/app.js",
    "watch": "npm-run-all --parallel bundle:watch start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/James-Lopez/react-typeahead.git"
  },
