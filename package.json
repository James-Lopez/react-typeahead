  "devDependencies": {
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "express": "^4.16.2",
    "mongodb": "^2.2.33",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.2",
    "watchify": "^3.9.0"
  },
  "dependencies": {
    "babel-core": "6.26.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.0",
    "babelify": "8.0.0",
    "browserify": "14.5.0",
    "express": "^4.16.2",
    "react": "^16.1.1",
    "react-dom": "^16.1.1"
  }
}
